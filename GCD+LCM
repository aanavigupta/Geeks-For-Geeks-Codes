#include <bits/stdc++.h>
//#include <cmath>
//#include <algorithm>
using namespace std;

int main() {
	int t;
	cin>>t;
long long	int a;
long long 	int b;
long long int gcd;
long long int lcm;
while(t--)
{
	cin>>a>>b;
	gcd=__gcd(a,b);
	lcm=(a*b)/gcd;
	cout<<lcm<<" "<<gcd;
	cout<<endl;
}
	
	return 0;
}
