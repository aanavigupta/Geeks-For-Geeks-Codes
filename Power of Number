/*package whatever //do not write package name here */

import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t--!=0)
		{
		    int x=sc.nextInt();
		    int y=sc.nextInt();
		    double d=Math.log(y)/Math.log(x);
		    if((int)d-d==0)
		    System.out.println("1");
		    else
		    System.out.println("0");
		}
	}
}
