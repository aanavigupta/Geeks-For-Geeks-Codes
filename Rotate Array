#include <iostream>
using namespace std;

int main() {
	long int t;
	cin>>t;
	long int n;
	long int d;
	while(t--)
	{
	    cin>>n;
	    long int arr[n];
	    cin>>d;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[(i+n-d)%n];
	    }
	    for(int i=0;i<n;i++)
	    {
	        cout<<arr[i]<<" ";
	    }
	
	    cout<<"\n";
	}
	
	return 0;
}
